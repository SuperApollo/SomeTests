<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="anim_top_bar">
        <item name="android:windowEnterAnimation">@anim/topbar_in</item>
        <item name="android:windowExitAnimation">@anim/topbar_out</item>
    </style>
    <declare-styleable name="CountDwonView">
        <attr name="ringColor" format="color" />
        <attr name="ringProgressColor" format="color" />
        <attr name="ringWidth" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="max" format="float" />
        <attr name="textIsShow" format="boolean" />
        <attr name="progress" format="float" />
        <attr name="ringPadding" format="dimension" />
        <attr name="style">
            <enum name="STROKE" value="0" />
            <enum name="FILL" value="1" />
        </attr>
    </declare-styleable>

</resources>
